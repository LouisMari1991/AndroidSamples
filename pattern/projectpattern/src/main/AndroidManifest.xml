<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sync.projectpattern">

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name=".mvc.LoadDataActivity"/>
    <activity android:name=".mvp.UserLoginActivity"/>
    <activity android:name=".mvpdatabinding.MvpDataBindingActivity"/>
    <activity android:name=".mvvm.ChangeAgeActivity"/>
  </application>

</manifest>